package com.company;

public class cwp_29_Practice_Set_6 {
    public static void main(String[] args) {
        float[] marks = {45.7f, 67.8f, 63.4f, 99.2f, 100.0f};
        float sum = 0;
        for (float element : marks) {
            sum = sum + element;
        }
        System.out.println("the value of average marks" + sum / marks.length);
    }
}
